{
  "extends": [
    "next",
    "eslint:recommended",
    "plugin:tailwindcss/recommended",
    "plugin:prettier/recommended",
    "plugin:jsx-a11y/strict"
  ],
  "rules": {
    "import/no-anonymous-default-export": "off",
    "no-unused-vars": "off",
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto"
      }
    ]
  },
  "plugins": ["tailwindcss", "prettier", "jsx-a11y"]
}
